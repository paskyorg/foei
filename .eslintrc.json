{
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true,
        "mocha": true,
        "webextensions": true
    },
    "plugins": [
        "mocha",
        "momentjs"
    ],
    "extends": "standard",
    "globals": {
        "addTab": "readonly",
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "battleField": "readonly",
        "cacheAction": "readonly",
        "cityMap": "readonly",
        "clanBattle": "readonly",
        "consts": "readonly",
        "copyProductResources": "readonly",
        "debug": "readonly",
        "fixFloat": "readonly",
        "greatBuilding": "readonly",
        "hiddenReward": "readonly",
        "humanReadableTime": "readonly",
        "i18n": "readonly",
        "iconImage": "readonly",
        "includeJavascript": "readonly",
        "listenToWorldIDChanged": "readonly",
        "listUnits": "readonly",
        "localGet": "readonly",
        "localSet": "readonly",
        "mapEqual": "readonly",
        "moment": "readonly",
        "msgCache": "readonly",
        "otherPlayer": "readonly",
        "resource": "readonly",
        "saveAs": "readonly",
        "sendMessageCache": "readonly",
        "sendNotification": "readonly",
        "sendPlayerArmies": "readonly",
        "setWorldID": "readonly",
        "sortByKey": "readonly",
        "sortByKeyMultiple": "readonly",
        "startup": "readonly",
        "syncGet": "readonly",
        "syncSet": "readonly",
        "trace": "readonly",
        "updateNotifications": "readonly",
        "updateSettingsWorlds": "readonly",
        "updatePlunder": "readonly",
        "updateRewards": "readonly",
        "updateBattleStats": "readonly",
        "updateClanArmies": "readonly",
        "updatePlayerProtection": "readonly",
        "updateGreatBuildingAnalysis": "readonly",
        "updateGreatBuildingBoostInfo": "readonly",
        "updateGreatBuildingChanges": "readonly",
        "worldID": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "off"
    }
}